.slide_img2 {
  height: 120px;
  width: 100%;
  display: inline-block;
  over-flow: hidden;
}
.time{
  display:flex;
  flex-direction: column;
  margin-top:30rpx;
  margin-bottom: 30rpx;
  align-items: center;
}
.startArea{
  display:flex;
  flex-direction: row;
}
.endArea{
  display:flex;
  flex-direction: row;
}
.search{
  margin-top:20rpx;
}
.section__title{
  margin-left: 50rpx;
  margin-top:30rpx;
  font-weight: bond;
  color: rgb(139, 135, 135);
}
picker{
  margin-left: 150rpx;
}
.picker{
  width: 100%;
  font-weight: bond;
  color: rgb(139, 135, 135);
}
.ranking_item{
  display: flex;
  flex-direction: row;
  justify-content: cnter;
  align-items: center;
  margin-top:30rpx;
}
.ranking_photo{
  width:20%;
  text-align: center;
}
.ranking_img{
  margin: 10rpx;
  width:80%;
  height:80%;
  border: 1px solid gainsboro;
  border-radius: 5px;
  box-shadow: 0px 2px 2px gainsboro;
}
.ranking_text{
  width:60%;
  display: flex;
  flex-direction: column;
  margin-left:30rpx;
}
.ranking_grade{
  font-size: 13px;
  font-weight: bold;
}
.ranking_title{
  font-size:10px;
  color:grey;
  margin:3px 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.ranking_opendate{
  color:red;
  font-size: 13px;
}
.ranking_zone{
  margin-left:20rpx;
  color:green;
  font-size:13px;
}
.ifdone{
  width:20%;
}
.yes{
  color: green;
}
.no{
  color: red;
}
.divLine{
 margin-top:20rpx;
 background: #E0E3DA;
 width: 100%;
 height: 5rpx;
}
.nothingArea{
  margin-left:50rpx;
  margin-top:50rpx;
}
.nothingArea_text{
  font-size: 50rpx;
}